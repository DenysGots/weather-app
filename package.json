  "scripts": {
    "ng": "ng",
    "start": "npm run serve:ssr",
    "lint": "ng lint nest-nguniversal-demo",
    "build": "ng build",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "build:ssr:local": "npm run build:client-and-server-bundles:local && npm run webpack:server",
    "build:client-and-server-bundles": "ng build --prod && ng run nest-nguniversal-demo:server:production",
    "build:client-and-server-bundles:local": "ng build && ng run nest-nguniversal-demo:server:local",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "generate:prerender": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "serve:prerender": "cd dist/browser && http-server -p 8080",
    "server:watch": "nodemon",
    "helper:build": "npm run build:ssr && npm run serve:ssr",
    "dev:local": "npm run build:ssr:local && npm run serve:ssr --configuration=local --open",
    "helper:test-build": "ng build && ng run nest-nguniversal-demo:server && npm run webpack:server && npm run serve:ssr && http-server",
    "heroku-postbuild": "npm run build:ssr",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "pre-commit": [],
